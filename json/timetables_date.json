---
---
{
    "items": [
            {% for item in site.data.talks %}{% if item.day == 'thursday' %}{
              "speaker": "{{ item.speaker }}",
              "image": "{{ item.speaker }}.jpg",
              "talk": "{{ item.name }}",
              "date": "{% if item.track == "main" %}20{% elsif item.track == "side1" %}21{% else %}22{% endif %}",
              "month": "11",
              "year": "2016",
              "start_time": "{{ item.starttime }}",
              "end_time": "{{ item.endtime }}",
              "type": "{{ item.type }}",
              "description": {{item.description | jsonify}}
            }{% unless forloop.last %},{% endunless %}
    {% endif %}{% endfor %}]
}